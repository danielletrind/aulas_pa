algoritmo "semnome"
// Autor :
var
numero:caracter
convertido,comprimento:inteiro
inicio
 escreval("Digite um número: ")
 leia(numero)
 convertido <- asc(numero)
 comprimento <- compr(numero)

 escreval("Numero digitado é: ",convertido)
 escreval("Comprimento Numero digitado é: ",comprimento)
fimalgoritmo
